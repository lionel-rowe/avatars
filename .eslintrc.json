{
	"extends": [],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"settings": {
		"react": {
			"pragma": "React",
			"version": "detect",
			"flowVersion": "0.53"
		}
	},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/explicit-function-return-type": "off"
			}
		}
	]
}
